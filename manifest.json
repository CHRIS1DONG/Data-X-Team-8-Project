{
    "version": "1.0",
    "manifest_version": 3,
    "name": "simple extension",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}
